{
  "version": 2,
  "builds": [
    {
      "src": "api/api_submit_form.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/submit-form",
      "dest": "api/api_submit_form.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
