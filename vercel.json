{
  "version": 2,
  "builds": [
    {
      "src": "api/submit-form.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/submit-form",
      "dest": "api/submit-form.py"
    }
  ],
  "env": {
    "TELEGRAM_BOT_TOKEN": "@telegram_bot_token",
    "TELEGRAM_CHAT_ID": "@telegram_chat_id"
  }
}
